{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    },
    "typeRoots": ["./node_modules/@types", "./src/express/types"]
  },
  "include": ["src/**/*"]
}
